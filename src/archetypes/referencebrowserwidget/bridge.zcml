<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="atreferencebrowserwidget">

  <browser:resource
    name="archetypes.referencebrowser.js"
    file="skins/referencebrowser/referencebrowser.js"
  />
  
  <adapter
    for="*"
    name="default"
    factory=".bridge.DefaultReferenceBrowserWidgetProps"
  />
  
  <adapter
    for="*"
    name="default"
    factory=".bridge.FieldDummy"
  />

</configure>