img.moveUp:click {
    action-server: moveUp;
    moveUp-pos: kssAttr(pos);   
    moveUp-length: kssAttr(length);
    moveUp-field: kssAttr(field);
}

img.moveDown:click {
    action-server: moveDown;
    moveDown-pos: kssAttr(pos);
    moveDown-length: kssAttr(length);
    moveDown-field: kssAttr(field);
}
